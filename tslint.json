{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "await-promise": [true, "Bluebird", "Thenable", "SchemaBuilder", "RequestPromise"],
    "no-floating-promises": [false, "Bluebird", "Thenable", "SchemaBuilder", "RequestPromise"],
    "promise-function-async": true,
    "ordered-imports": false,
    "no-console": true,
    "comment-format": false,
    "no-var-requires": false,
    "interface-name": false,
    "no-implicit-dependencies": false,
    "max-classes-per-file": false,
    "no-duplicate-imports": false,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-empty-interface": false,
    "no-switch-case-fall-through": true,
    "object-literal-sort-keys": false,
    "member-access": false,
    "member-ordering": false,
    "strict-type-predicates": true,
    "strict-boolean-expressions": [false, "allow-null-union", "allow-undefined-union", "ignore-rhs"],
    "prefer-for-of": false,
    "object-literal-shorthand": false
  }
}
